<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog>
		<beginButton>
			<Button text="Cancel" press="onCancel" type="Reject"/>
		</beginButton>
		<endButton>
			<Button text="Save" press="onAddPlaceofSafetyItem" type="Accept"/>
		</endButton>
		<content>
<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout">
    <sap.ui.layout.form:content>
        <core:Title text="Place of Safety (Retension Orders)" icon="sap-icon://account"/>
        <Label text="Surname &amp; Initials/ File Number"/>
        <Input width="100%" id="posSurname"/>
        <Label text="Date of Birth/ ID Number"/>
        <Input width="100%" id="posDoB"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Date Admitted"/>
        <DatePicker xmlns="sap.m" id="posDoA" displayFormat="dd-MM-YYYY"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Age"/>
        <Input width="100%"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Gender"/>
        <ComboBox xmlns="sap.m">
            <items>
                <core:Item xmlns:core="sap.ui.core" key="item-1-key" text="No" id="item18"/>
                <core:Item xmlns:core="sap.ui.core" key="item-2-key" text="Yes" id="item19"/>
                </items>
            
        </ComboBox>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Race"/>
        <ComboBox xmlns="sap.m">
            <items>
                <core:Item xmlns:core="sap.ui.core" key="item-1-key" text="Black" id="item34"/>
                <core:Item xmlns:core="sap.ui.core" key="item-2-key" text="White" id="item35"/>
                <core:Item xmlns="sap.m" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:core="sap.ui.core" key="item-2-key" text="Coloured" id="item35_copy"/>
                <core:Item xmlns="sap.m" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:core="sap.ui.core" key="item-2-key" text="Asian" id="item35_copy2"/>
                </items>
            
        </ComboBox>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Disabled"/>
        <ComboBox xmlns="sap.m">
            <items>
                <core:Item xmlns:core="sap.ui.core" key="item-1-key" text="No" id="item73"/>
                <core:Item xmlns:core="sap.ui.core" key="item-2-key" text="Yes" id="item74"/>
                </items>
            
        </ComboBox>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Specify Disability"/>
        <Input xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" width="100%"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Admissions"/>
        <CheckBox xmlns="sap.m" text="New Admission" selected="false" wrapping="true"/>
        <CheckBox xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns="sap.m" text="Transferred from Place of Safety" selected="false" wrapping="true"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Date of Exit"/>
        <DatePicker xmlns="sap.m" displayFormat="dd-MM-YYYY"/>
        <CheckBox xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns="sap.m" text="Court Order" selected="false" wrapping="true"/>
        <CheckBox xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns="sap.m" text="Reunified with Family" selected="false" wrapping="true"/>
        <CheckBox xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns="sap.m" text="Absconded" selected="false" wrapping="true"/>
        <CheckBox xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns="sap.m" text="Foster Care" selected="false" wrapping="true"/>
        <Label xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" text="Other (Specify)"/>
        <Input xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" width="100%"/>
        </sap.ui.layout.form:content>
</sap.ui.layout.form:SimpleForm>
</content>
</Dialog>
</core:FragmentDefinition>